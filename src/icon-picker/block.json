{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/icon-picker",
	"version": "0.1.0",
	"title": "Icon Picker",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"iconType": {
			"type": "string",
			"default": "library"
		},
		"selectedIcon": {
			"type": "string",
			"default": "AArrowDown"
		},
		"iconUrl": {
			"type": "string",
			"default": ""
		},
		"iconSize": {
			"type": "number",
			"default": 50
		},
		"iconColor": {
			"type": "string",
			"default": "#000000"
		}
	},
	"textdomain": "icon-picker",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
